{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
  "manifestVersion": "1.9",
  "version": "1.0.0",
  "id": "00000000-0000-0000-0000-000000000000",
  "packageName": "com.microsoft.teams.resourcetracker",
  "developer": {
    "name": "Microsoft Inc",
    "websiteUrl": "https://www.teams.com",
    "privacyUrl": "https://www.teams.com/privacy",
    "termsOfUseUrl": "https://www.teams.com/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "MS Teams React App",
    "full": "An MS Teams React App Template"
  },
  "description": {
    "short": "MS Teams React App",
    "full": "MS Teams React Application"
  },
  "accentColor": "#FFFFFF",
  "configurableTabs": [
    {
      "configurationUrl": "https://<app service domain>/config",
      "canUpdateConfiguration": true,
      "scopes": ["team"],
      "context": ["channelTab"]
    }
  ],
  "permissions": ["identity", "messageTeamMembers"],
  "validDomains": ["<app service domain>"],
  "webApplicationInfo": {
    "id": "<client id>",
    "resource": "api://<app service domain>"
  }
}
